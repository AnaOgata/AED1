#include <stdio.h>

/*Escreva a documentação correta da função abaixo.
    int soma (int n, int v[]) {
        int i, x = 0;
        for (i = 0; i < n; i++) x += v[i];
        return x; }*/

/*A função abaixo recebe um valor n >= 1 e um vetor v e retorna a soma de todos os elementos de v[0...n-1]*/
int soma(int n, int v[])
{
    int i, x = 0;
        for (i = 0; i < n; i++) x += v[i];
        return x;
}
